<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Your Website Title</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="css/style.css">
</head>

<body>

    <!-- Header -->
    <header class="bg-prime text-white">
        <div class="container-fluid">
            <%- include("partial/header.ejs") %>
        </div>
    </header>

    <!-- Main Content -->
    <main>
        <div class="container-fluid">
            <div class="row">
                <!-- Sidebar -->
                <aside class="col-lg-3 col-md-12 col-sm-12 bg-light border-end">
                    <%- include("partial/sidebar.ejs") %>
                </aside>
                <div class="col-lg-1 col-md-0 col-sm-0">

                </div>

                <!-- Main Content Area -->
                <div class="col-lg-7 col-md-12 col-sm-12">
                    <% if (locals.data) { %>
                        <% for (const key in data) { %>
                            <% if (data.hasOwnProperty(key)) { %>
                                <% const blog=data[key]; %>
                                    <h2>
                                        <%= blog.topic %>
                                    </h2>
                                    <p>
                                        <%= blog.content %>
                                    </p>
                                    <h6>
                                        <%= blog.created.uName %>
                                        <strong>(<%= blog.created.uEmail  %>)</strong>
                                    </h6>
                                    <% } %>
                                        <% } %>
                                            <% } %>
                </div>
            </div>
        </div>
    </main>

    <!-- Footer -->
    <footer class="bg-prime text-white">
        <div class="container-fluid">
            <%- include("partial/footer.ejs") %>
        </div>
    </footer>

    <!-- Bootstrap Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
        crossorigin="anonymous"></script>
</body>

</html>